{
  "name": "poptavky-fe",
  "version": "0.1.0",
  "private": true,
  "description": "A frontend to display GitHub issues in a webpage",
  "keywords": [
    "react",
    "issues"
  ],
  "homepage": "https://itpoptavky.skaut.cz",
  "bugs": {
    "url": "https://github.com/skaut/poptavky/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skaut/poptavky.git"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "prebuild": "npm run clean",
    "build:public": "mkdir -p dist && cp -r public/* dist/",
    "build:webpack": "webpack",
    "build": "run-p -c --aggregate-output build:*",
    "lint": "eslint --color webpack.config.js 'src/**/*' '__mocks__/**/*' '__tests__/**/*'",
    "test": "jest"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-markdown": "^8.0.7",
    "react-router-dom": "^6.14.2",
    "remark-gfm": "^3.0.1",
    "string-to-color": "^2.2.2",
    "swr": "^2.2.0"
  },
  "devDependencies": {
    "@emotion/jest": "^11.11.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/react-dom": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "babel-preset-react-app": "^10.0.1",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-deprecation": "^1.4.1",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-mock": "^29.6.1",
    "mini-css-extract-plugin": "^2.4.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^5.0.1",
    "terser-webpack-plugin": "^5.2.5",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
