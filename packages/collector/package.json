{
  "name": "poptavky",
  "version": "0.1.0",
  "private": true,
  "description": "A GitHub app to automatically export issues to a webpage.",
  "keywords": [
    "plugin",
    "github app",
    "automation"
  ],
  "homepage": "https://github.com/skaut/poptavky#readme",
  "bugs": {
    "url": "https://github.com/skaut/poptavky/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skaut/poptavky.git"
  },
  "author": "Marek Dědič",
  "type": "module",
  "scripts": {
    "collect": "node --experimental-strip-types src/index.ts",
    "lint": "run-p -c --aggregate-output lint:*",
    "lint:eslint": "eslint --color",
    "lint:typecheck": "tsc --noEmit",
    "test": "vitest",
    "test-coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@actions/core": "^2.0.2",
    "@octokit/core": "^7.0.6",
    "@octokit/plugin-rest-endpoint-methods": "^17.0.0",
    "ts-custom-error": "^3.3.1"
  },
  "devDependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.6.0",
    "@eslint/js": "^9.39.2",
    "@eslint/json": "^0.14.0",
    "@types/node": "^25.0.9",
    "@vitest/coverage-v8": "^4.0.17",
    "@vitest/eslint-plugin": "^1.6.6",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-package-json": "^0.88.1",
    "eslint-plugin-perfectionist": "^5.3.1",
    "eslint-plugin-prefer-arrow-functions": "^3.9.1",
    "eslint-plugin-prettier": "^5.5.5",
    "nock": "^14.0.10",
    "node-fetch": "^3.3.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.8.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1",
    "vitest": "^4.0.18"
  }
}
